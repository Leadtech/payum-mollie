{
    "name": "sourcefabric/payum-mollie",
    "type": "library",
    "description": "The Payum extension. It provides Mollie payment integration.",
    "keywords": ["payment", "payum", "mollie", "sepa", "directdebit", "creditcard"],
    "homepage": "https://sourcefabric.org",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Rafał Muszyński",
            "email": "rafal.muszynski@sourcefabric.org"
        }
    ],
    "require": {
        "php": "^7.1",
        "payum/core": "^1.3",
        "mollie/mollie-api-php": "^1.9"
    },
    "require-dev": {
        "payum/core": "^1.3@dev",
        "phpspec/phpspec": "^4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Sourcefabric\\Payum\\Mollie\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": { "Sourcefabric\\Payum\\Mollie\\spec\\": "spec/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
